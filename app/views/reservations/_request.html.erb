<tr id="request-<%= request.id %>">
  <td><%= request.start_date.to_formatted_s(:short) %></td>
  <td><%= request.end_date.to_formatted_s(:short) %></td>
  <td><%= link_to request.renter.full_name, request.renter %></td>
  <td><%= number_to_currency request.total_price %></td>
  <td><%= render partial: "reservations/requests/action_#{request.status}", locals: {request: request} %></td>
</tr>
